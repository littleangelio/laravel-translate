{
    "name": "littleangel/laravel-translate",
    "description": "原地址：https://github.com/ShaoZeMing/laravel-translate,只是为了项目单纯使用",
    "type": "library",
    "require": {
        "shaozeming/translate": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^0.9.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "LittleAngel",
            "email": "littleangel@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LittleAngel\\LaravelTranslate\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LittleAngel\\LaravelTranslate\\Test\\": "./tests"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "LittleAngel\\LaravelTranslate\\TranslateServiceProvider"
            ],
            "aliases": {
                "Translate": "LittleAngel\\LaravelTranslate\\Facade\\Translate"
            }
        }
    },
    "minimum-stability":"dev"

}
